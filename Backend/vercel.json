{
    "version": 2,
    "builds": [{"src": "./index.js", "use":"@vercel/node"}],
    "routes": [{"src": "/(.*)", "dest":"/"}],
    "rewrites": [
        { "src": "/signup", "dest": "/index.js" },
        { "src": "/login", "dest": "/index.js" },
        { "src": "/:id", "dest": "/index.js" },
        { "src": "/todo", "dest": "/index.js" },
        { "src": "/(.*)", "dest": "/" }
  ]
}