{
    "version": 2,
    "builds": [{"src": "./index.js", "use":"@vercel/node"}],
    "routes": [{"src": "/(.*)", "dest":"/"}],
    "rewrites": [
        { "source": "/signup", "destination": "/index.js" },
        { "source": "/login", "destination": "/index.js" },
        { "source": "/:id", "destination": "/index.js" },
        { "source": "/todo", "desinationt": "/index.js" },
        { "source": "/(.*)", "destination": "/" }
  ]
}